# Database Configuration
# Default URL matches the docker-compose postgres service
DATABASE_URL=jdbc:postgresql://localhost:5432/waterballsa
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=postgres

# JWT Configuration
# Change this secret in production!
JWT_SECRET=your-256-bit-secret-key-for-jwt-token-generation-please-change-in-production
# 15 minutes in milliseconds
JWT_ACCESS_TOKEN_EXPIRATION=900000
# 7 days in milliseconds
JWT_REFRESH_TOKEN_EXPIRATION=604800000

# Google OAuth Configuration
GOOGLE_OAUTH_CLIENT_ID=dummy-client-id
